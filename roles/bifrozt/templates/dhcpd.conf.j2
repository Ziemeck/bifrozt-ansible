authoritative;
ddns-update-style none;
log-facility local7;
default-lease-time 600;
max-lease-time 7200;

subnet 10.19.0.0 netmask 255.255.0.0 {
    option domain-name "prod";
    option domain-name-servers 8.8.8.8;
    option subnet-mask 255.255.0.0;
    option broadcast-address 10.19.255.255;
    option routers 10.19.0.1;
    range 10.19.20.21 10.19.58.19;

	host sestbrlx05p {
  		hardware ethernet 94:10:3e:b4:ee:c8;
  		fixed-address 10.19.58.19;
		}

}

